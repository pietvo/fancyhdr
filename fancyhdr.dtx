% \noindent\makebox[\textwidth]{4\phantom{To: Dean A. Smith}\hfill
%  From: K. Grant\hfill \phantom{4}To: Dean A. Smith}
%
% \end{fblock}
% \noindent where ``4'' is the page number.
%
