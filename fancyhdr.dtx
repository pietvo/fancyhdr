% #1=current section label #2=ref section label
\newcommand{\smartref}[2]{%
  \setcounterref{secnum}{#1}%
  \addtocounter{secnum}{-1}%
  \ifnum\value{secnum}=\getrefnumber{#2}
    the previous section%
  \else
    \addtocounter{secnum}{2}%
    \ifnum\value{secnum}=\getrefnumber{#2}
      the next section%
    \else
      section~\ref{#2}%
    \fi
  \fi
}

