% The selectors may be given as uppercase or lowercase letters.
% \begin{figure}[htb]
%   \begin{center}
%     \leavevmode
%     \begin{tabular}{|l|l|}
%       \hline
%       E & Even page         \\
%       O & Odd page          \\
%       \hline
%       L & Left field        \\
%       C & Center field      \\
%       R & Right field       \\
%       \hline
%       H & Header            \\
%       F & Footer            \\
%       \hline
%     \end{tabular}
%   \end{center}
%   \caption{Selectors}
%   \label{fig:sel}
% \end{figure}
